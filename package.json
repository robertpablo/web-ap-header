{
  "name": "web-ap-header",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:env": "tsc --project tsconfig.env.json",
    "update-scss": "node scripts/update-scss-variables.js",
    "prestart": "npm run build:env && npm run update-scss",
    "start": "npm run prestart && ng serve --configuration=localhost",
    "prebuild": "npm run build:env && npm run update-scss",
    "build": "ng build",
    "build:prod": "npm run prebuild && ng build --prod",
    "build:dev": "npm run prebuild && ng build --dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ng-select/ng-select": "^12.0.7",
    "@ngxs/devtools-plugin": "^3.8.2",
    "@ngxs/logger-plugin": "^3.8.2",
    "@ngxs/store": "^3.8.2",
    "@ropabajo/core": "^0.1.7",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "feather-icons": "^4.29.1",
    "ngx-logger": "^5.0.12",
    "rxjs": "~7.8.0",
    "single-spa-angular": "^9.0.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
